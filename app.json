{
  "expo": {
    "name": "CivicVault",
    "slug": "civic-vault",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "civicvault",
    "icon": "./assets/images/logo-light-1.jpg.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "image": "./assets/images/darksplash.jpg.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-light-1.jpg.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "splash-screen": {
            "resizeMode": "contain",
            "backgroundColor": "#FFFFFF",
            "dark": {
              "backgroundColor": "#000000",
              "resizeMode": "contain"
            }
          }
        }
      ]
    ]
  }
}
